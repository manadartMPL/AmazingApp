<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="mwoo.amazingapp.MainMenu"
    tools:showIn="@layout/activity_main_menu"
    android:clickable="true">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/create_maze"
        android:src="@drawable/create_maze"
        android:layout_marginTop="163dp"
        android:adjustViewBounds="false"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:onClick="clickedCreate"/>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/choose_maze"
        android:src="@drawable/start_maze"
        android:layout_marginTop="26dp"
        android:layout_below="@+id/create_maze"
        android:layout_alignLeft="@+id/create_maze"
        android:layout_alignStart="@+id/create_maze"
        android:onClick ="clickedChoose"/>
</RelativeLayout>
